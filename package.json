{
  "name": "aicamp-ai-diagnosis-vercel",
  "version": "3.1.0",
  "description": "AICAMP AI 역량진단 시스템 - V10.0 PREMIUM 완전 오류 수정 버전",
  "main": "src/app/api/ai-diagnosis/route.ts",
  "scripts": {
    "dev": "next dev",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true next build",
    "build:win": "set DISABLE_ESLINT_PLUGIN=true && next build",
    "build:vercel": "cross-env DISABLE_ESLINT_PLUGIN=true NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "node test/health-check.js",
    "test:e2e": "node test/ai-diagnosis-workflow-simulation.mjs",
    "test:e2e:mckinsey": "node test/mckinsey-report-e2e-simulation.mjs",
    "test:server": "node test-server.js",
    "test:client": "node test-client.js",
    "test:api": "node test-api.js",
    "test:quick": "test-quick.bat",
    "test:cmd": "cmd /c \"node test/mckinsey-report-e2e-simulation.mjs\"",
    "dev:cmd": "cmd /c \"npm run dev\"",
    "build:cmd": "cmd /c \"npm run build\"",
    "lint": "next lint",
    "health": "curl -X GET https://aicamp-ai-diagnosis.vercel.app/api/health/check",
    "test:comprehensive": "node comprehensive-workflow-test.js",
    "test:fix": "node fix-environment-setup.js",
    "test:workflow": "node test-ai-diagnosis-complete.js",
    "dev:test": "npm run dev & timeout 30 && npm run test:comprehensive"
  },
  "keywords": [
    "ai",
    "diagnosis",
    "business",
    "gemini",
    "vercel",
